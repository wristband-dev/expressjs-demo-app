{
  "name": "expressjs-demo-app--backend",
  "description": "Express backend server that handles all API requests coming from the React frontend.",
  "author": "Wristband",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=18.12.1",
    "npm": ">=8.19.2"
  },
  "main": "index.ts",
  "scripts": {
    "build": "tsc",
    "dev": "nodemon",
    "typecheck": "tsc --noEmit",
    "reset-db": "ts-node reset-db.ts",
    "start": "NODE_ENV=production PORT=6001 tsx -r dotenv/config index.ts",
    "lint": "eslint . --ext .js --ext .ts",
    "lint:fix": "eslint . --ext .js --ext .ts --fix"
  },
  "dependencies": {
    "@wristband/express-auth": "^5.0.0",
    "@wristband/typescript-jwt": "^0.1.0",
    "agentkeepalive": "^4.6.0",
    "axios": "^1.13.2",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "dotenv": "^17.2.2",
    "express": "^5.1.0",
    "express-validator": "^7.2.1",
    "lowdb": "^1.0.0",
    "morgan": "^1.10.1",
    "stoppable": "^1.1.0",
    "validator": "^13.15.15"
  },
  "devDependencies": {
    "@types/compression": "^1.8.1",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/lowdb": "^1.0.15",
    "@types/morgan": "^1.9.10",
    "@types/node": "^24.8.1",
    "@types/stoppable": "^1.1.3",
    "@types/validator": "^13.15.3",
    "@typescript-eslint/eslint-plugin": "^8.46.1",
    "@typescript-eslint/parser": "^8.46.1",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "nodemon": "^3.1.10",
    "prettier": "^3.2.4",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3"
  }
